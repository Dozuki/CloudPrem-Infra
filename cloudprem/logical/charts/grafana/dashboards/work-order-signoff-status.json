          "alias": "",
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT wl.workorderid, \nwl.guideid, \ngt.title, \nso.entryid, \n-- wl.userid, \nu.username, \n-- so.signoffid, \n-- so.stepid, \n-- gr.guide_revisionid, \n-- gr.gst_revisionid, \n-- gstt.newest_gst_revisionid, \ngtr.orderby, \ngst.title as \"Step Title\", \n\nCONVERT_TZ(from_unixtime(so.date), @@session.time_zone, '+00:00') as \"Date\", \n\n\nCASE \n  WHEN so.status = \"approved\" THEN \"1\"\n  WHEN so.status = \"pending\"  THEN \"0\"\n  WHEN so.status = \"rejected\" THEN \"-1\"\nEND as \"Status\", \n\n-- so.status, \n\n\nso.type, \n-- su.so_userid, \nsu.so_username,  \n-- gm.device, \n\"PDF Log\" as \"PDF Log\"\n\nfrom supervisor_signoffs so\nleft join work_log wl on wl.entryid=so.entryid \n\n\nleft join guide_revisions gr on gr.guide_revisionid=wl.guide_revisionid\n\nleft join guide_meta gm on gm.revisionid=gr.gtr_revisionid\nleft join guide_text gt on gt.gt_revisionid=gr.gt_revisionid\n\n-- Add step titles\n\n\n\nleft JOIN (SELECT stepid, max(gst_revisionid) as newest_gst_revisionid\nFROM guide_step_text\ngroup by stepid order by newest_gst_revisionid) gstt on gstt.stepid=so.stepid\n\nleft JOIN guide_step_text gst on gst.gst_revisionid=gstt.newest_gst_revisionid\n\n\n-- Add user IDs\n\nleft join global_users u on u.userid=wl.userid \n\n-- Add user names\n\nLEFT join (select sso.userid  as so_userid, ssu.username as so_username, sso.signoffid as so_signoffid \n  from supervisor_signoffs sso\n  left join global_users ssu on sso.userid=ssu.userid ) su on so.signoffid=su.so_signoffid\n\n-- Add step numbers\n\n\n\n\n\nLEFT JOIN (SELECT orderby, stepid, max(gtr_revisionid) as newest_gtr_revisionid\n\nFROM guide_step_order_revisions\n\t\t   \ngroup by stepid order by newest_gtr_revisionid ) gsor on gsor.stepid=so.stepid \n\nleft JOIN guide_step_order_revisions gtr on gtr.gtr_revisionid =gsor.newest_gtr_revisionid and gtr.stepid=gsor.stepid \n\n\n  \n-- Conditions for filters\n\nWHERE\n($WORKORDERID = 'All Work Orders' OR wl.workorderid = $WORKORDERID)\n\t\nAND\n($GUIDE_ID = 'All Guides' OR gm.guideid = $GUIDE_ID)\n\n\n-- group by so.signoffid",
